{
  "title": "Configs",
  "description": "Configuration file for synthetic data generation",
  "type": "object",
  "required": ["io", "synth_generator"],
  "properties": {
    "io": {
      "type": "object",
      "required": ["out_folder"],
      "properties": {
        "out_folder": {
          "type": "string"
        }
      }
    },
    "synth_generator": {
      "type": "object",
      "required": ["total_objects", "test_percentage", "validation_percentage"],
      "properties": {
        "total_objects": {
          "type": "number",
          "minimum": 50,
          "maximum": 10000
        },
        "test_percentage": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        },
        "validation_percentage": {
          "type": "number",
          "minimum": 0,
          "maximum": 1
        }
      }
    }
  }
}
